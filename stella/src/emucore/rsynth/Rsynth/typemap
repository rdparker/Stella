TYPEMAP
rsynth_t *  	T_RSYNTH
Elm_t *		T_ELM

INPUT
T_RSYNTH
	if (sv_isobject($arg)) {
	    $var = ($type) SvIV((SV*)SvRV($arg));
	}
	else
	    croak(\"$var is not an object\")

T_ELM
	$var = ($type) SVtoElm(aTHX_ $arg);

OUTPUT
T_RSYNTH
    sv_setref_iv($arg, (Class) ? Class : \"${Package}\", (IV) $var);

T_ELM
    ElmToSV(aTHX_ $arg, $var);

